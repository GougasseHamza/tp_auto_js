{"ast":null,"code":"import \"core-js/modules/es.array.push.js\";\nimport \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.reduce.js\";\nimport \"core-js/modules/es.set.difference.v2.js\";\nimport \"core-js/modules/es.set.intersection.v2.js\";\nimport \"core-js/modules/es.set.is-disjoint-from.v2.js\";\nimport \"core-js/modules/es.set.is-subset-of.v2.js\";\nimport \"core-js/modules/es.set.is-superset-of.v2.js\";\nimport \"core-js/modules/es.set.symmetric-difference.v2.js\";\nimport \"core-js/modules/es.set.union.v2.js\";\nexport default {\n  data() {\n    return {\n      products: [],\n      cart: [],\n      searchQuery: \"\",\n      selectedCategory: \"\",\n      selectedAvailability: \"\",\n      sortOrder: \"asc\"\n    };\n  },\n  computed: {\n    categories() {\n      return [...new Set(this.products.map(product => product.categorie))];\n    },\n    filteredProducts() {\n      return this.products.filter(product => {\n        const matchesCategory = !this.selectedCategory || product.categorie === this.selectedCategory;\n        const matchesAvailability = this.selectedAvailability === \"\" || product.disponible.toString() === this.selectedAvailability;\n        const matchesSearch = product.nom.toLowerCase().includes(this.searchQuery.toLowerCase());\n        return matchesCategory && matchesAvailability && matchesSearch;\n      }).sort((a, b) => {\n        return this.sortOrder === \"asc\" ? a.prix - b.prix : b.prix - a.prix;\n      });\n    },\n    totalPrice() {\n      return this.cart.reduce((acc, item) => acc + item.prix, 0);\n    }\n  },\n  methods: {\n    async fetchProducts() {\n      const response = await fetch(\"/path/to/pieces-auto.json\");\n      this.products = await response.json();\n    },\n    addToCart(product) {\n      this.cart.push(product);\n    },\n    removeFromCart(productId) {\n      this.cart = this.cart.filter(item => item.id !== productId);\n    }\n  },\n  mounted() {\n    this.fetchProducts();\n  }\n};","map":{"version":3,"names":["data","products","cart","searchQuery","selectedCategory","selectedAvailability","sortOrder","computed","categories","Set","map","product","categorie","filteredProducts","filter","matchesCategory","matchesAvailability","disponible","toString","matchesSearch","nom","toLowerCase","includes","sort","a","b","prix","totalPrice","reduce","acc","item","methods","fetchProducts","response","fetch","json","addToCart","push","removeFromCart","productId","id","mounted"],"sources":["/home/kyyblin/tp-auto/src/components/ProductList.vue"],"sourcesContent":["<template>\n    <div>\n      <h1>Catalogue de Pièces Auto</h1>\n  \n      <!-- Search bar -->\n      <input\n        type=\"text\"\n        v-model=\"searchQuery\"\n        placeholder=\"Rechercher une pièce...\"\n        class=\"search-bar\"\n      />\n  \n      <!-- Filters -->\n      <div>\n        <label for=\"category-filter\">Filtrer par catégorie: </label>\n        <select v-model=\"selectedCategory\" id=\"category-filter\">\n          <option value=\"\">Tous</option>\n          <option v-for=\"category in categories\" :key=\"category\" :value=\"category\">{{ category }}</option>\n        </select>\n  \n        <label for=\"available-filter\">Disponibilité: </label>\n        <select v-model=\"selectedAvailability\" id=\"available-filter\">\n          <option value=\"\">Tous</option>\n          <option value=\"true\">Disponible</option>\n          <option value=\"false\">Indisponible</option>\n        </select>\n      </div>\n  \n      <!-- Sort by price -->\n      <div>\n        <label for=\"sort-price\">Trier par prix: </label>\n        <select v-model=\"sortOrder\" id=\"sort-price\">\n          <option value=\"asc\">Croissant</option>\n          <option value=\"desc\">Décroissant</option>\n        </select>\n      </div>\n  \n      <!-- Product List -->\n      <div class=\"product-list\">\n        <div v-for=\"product in filteredProducts\" :key=\"product.id\" class=\"product-item\">\n          <img :src=\"`/images/${product.image}`\" alt=\"product image\" class=\"product-image\" />\n          <div class=\"product-details\">\n            <h2>{{ product.nom }}</h2>\n            <p>{{ product.categorie }}</p>\n            <p>Prix: {{ product.prix }}€</p>\n            <p v-if=\"product.disponible\">En stock</p>\n            <p v-else>Indisponible</p>\n            <button @click=\"addToCart(product)\" :disabled=\"!product.disponible\">Ajouter au panier</button>\n          </div>\n        </div>\n      </div>\n  \n      <!-- Cart -->\n      <h2>Mon Panier</h2>\n      <div v-if=\"cart.length > 0\">\n        <ul>\n          <li v-for=\"item in cart\" :key=\"item.id\">\n            {{ item.nom }} - {{ item.prix }}€\n            <button @click=\"removeFromCart(item.id)\">Retirer</button>\n          </li>\n        </ul>\n        <p>Total: {{ totalPrice }}€</p>\n      </div>\n      <div v-else>\n        <p>Votre panier est vide.</p>\n      </div>\n    </div>\n  </template>\n  \n  <script>\n  export default {\n    data() {\n      return {\n        products: [],\n        cart: [],\n        searchQuery: \"\",\n        selectedCategory: \"\",\n        selectedAvailability: \"\",\n        sortOrder: \"asc\",\n      };\n    },\n    computed: {\n      categories() {\n        return [...new Set(this.products.map(product => product.categorie))];\n      },\n      filteredProducts() {\n        return this.products\n          .filter(product => {\n            const matchesCategory =\n              !this.selectedCategory || product.categorie === this.selectedCategory;\n            const matchesAvailability =\n              this.selectedAvailability === \"\" || product.disponible.toString() === this.selectedAvailability;\n            const matchesSearch =\n              product.nom.toLowerCase().includes(this.searchQuery.toLowerCase());\n  \n            return matchesCategory && matchesAvailability && matchesSearch;\n          })\n          .sort((a, b) => {\n            return this.sortOrder === \"asc\"\n              ? a.prix - b.prix\n              : b.prix - a.prix;\n          });\n      },\n      totalPrice() {\n        return this.cart.reduce((acc, item) => acc + item.prix, 0);\n      },\n    },\n    methods: {\n      async fetchProducts() {\n        const response = await fetch(\"/path/to/pieces-auto.json\");\n        this.products = await response.json();\n      },\n      addToCart(product) {\n        this.cart.push(product);\n      },\n      removeFromCart(productId) {\n        this.cart = this.cart.filter(item => item.id !== productId);\n      },\n    },\n    mounted() {\n      this.fetchProducts();\n    },\n  };\n  </script>\n  \n  <style scoped>\n  /* Add styling for the components here */\n  </style>\n  "],"mappings":";;;;;;;;;;;;AAsEE,eAAe;EACbA,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,QAAQ,EAAE,EAAE;MACZC,IAAI,EAAE,EAAE;MACRC,WAAW,EAAE,EAAE;MACfC,gBAAgB,EAAE,EAAE;MACpBC,oBAAoB,EAAE,EAAE;MACxBC,SAAS,EAAE;IACb,CAAC;EACH,CAAC;EACDC,QAAQ,EAAE;IACRC,UAAUA,CAAA,EAAG;MACX,OAAO,CAAC,GAAG,IAAIC,GAAG,CAAC,IAAI,CAACR,QAAQ,CAACS,GAAG,CAACC,OAAM,IAAKA,OAAO,CAACC,SAAS,CAAC,CAAC,CAAC;IACtE,CAAC;IACDC,gBAAgBA,CAAA,EAAG;MACjB,OAAO,IAAI,CAACZ,QAAO,CAChBa,MAAM,CAACH,OAAM,IAAK;QACjB,MAAMI,eAAc,GAClB,CAAC,IAAI,CAACX,gBAAe,IAAKO,OAAO,CAACC,SAAQ,KAAM,IAAI,CAACR,gBAAgB;QACvE,MAAMY,mBAAkB,GACtB,IAAI,CAACX,oBAAmB,KAAM,EAAC,IAAKM,OAAO,CAACM,UAAU,CAACC,QAAQ,CAAC,MAAM,IAAI,CAACb,oBAAoB;QACjG,MAAMc,aAAY,GAChBR,OAAO,CAACS,GAAG,CAACC,WAAW,CAAC,CAAC,CAACC,QAAQ,CAAC,IAAI,CAACnB,WAAW,CAACkB,WAAW,CAAC,CAAC,CAAC;QAEpE,OAAON,eAAc,IAAKC,mBAAkB,IAAKG,aAAa;MAChE,CAAC,EACAI,IAAI,CAAC,CAACC,CAAC,EAAEC,CAAC,KAAK;QACd,OAAO,IAAI,CAACnB,SAAQ,KAAM,KAAI,GAC1BkB,CAAC,CAACE,IAAG,GAAID,CAAC,CAACC,IAAG,GACdD,CAAC,CAACC,IAAG,GAAIF,CAAC,CAACE,IAAI;MACrB,CAAC,CAAC;IACN,CAAC;IACDC,UAAUA,CAAA,EAAG;MACX,OAAO,IAAI,CAACzB,IAAI,CAAC0B,MAAM,CAAC,CAACC,GAAG,EAAEC,IAAI,KAAKD,GAAE,GAAIC,IAAI,CAA