{"ast":null,"code":"import \"core-js/modules/es.iterator.constructor.js\";\nimport \"core-js/modules/es.iterator.filter.js\";\nimport \"core-js/modules/es.iterator.map.js\";\nimport \"core-js/modules/es.iterator.reduce.js\";\nimport \"core-js/modules/es.set.difference.v2.js\";\nimport \"core-js/modules/es.set.intersection.v2.js\";\nimport \"core-js/modules/es.set.is-disjoint-from.v2.js\";\nimport \"core-js/modules/es.set.is-subset-of.v2.js\";\nimport \"core-js/modules/es.set.is-superset-of.v2.js\";\nimport \"core-js/modules/es.set.symmetric-difference.v2.js\";\nimport \"core-js/modules/es.set.union.v2.js\";\n// Importez les données depuis le fichier JSON\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      pieces: [{}],\n      searchText: \"\",\n      selectedCategory: \"\",\n      filterDisponible: true,\n      // Affiche par défaut uniquement les pièces disponibles\n      sortOrder: \"asc\",\n      panier: []\n    };\n  },\n  mounted() {\n    fetch('./asset/pieces-auto.json').then(response => response.json()).then(data => {\n      console.log(data);\n      this.pieces = data;\n    }).catch(error => console.error(\"Erreur de chargement des pièces:\", error));\n  },\n  computed: {\n    // Liste des catégories uniques pour le filtre\n    categories() {\n      const set = new Set(this.pieces.map(piece => piece.categorie));\n      return Array.from(set);\n    },\n    // Filtrage des pièces en fonction de la recherche, de la catégorie et de la disponibilité\n    filteredPieces() {\n      return this.pieces.filter(piece => {\n        // Filtre sur la disponibilité\n        if (this.filterDisponible && !piece.disponible) return false;\n\n        // Filtre sur la barre de recherche (insensible à la casse)\n        if (this.searchText && !piece.nom.toLowerCase().includes(this.searchText.toLowerCase())) {\n          return false;\n        }\n\n        // Filtre sur la catégorie sélectionnée\n        if (this.selectedCategory && piece.categorie !== this.selectedCategory) {\n          return false;\n        }\n        return true;\n      });\n    },\n    // Tri des pièces filtrées par prix\n    sortedPieces() {\n      return this.filteredPieces.slice().sort((a, b) => {\n        return this.sortOrder === \"asc\" ? a.prix - b.prix : b.prix - a.prix;\n      });\n    },\n    // Calcul du total du panier\n    totalPanier() {\n      return this.panier.reduce((total, piece) => total + piece.prix, 0);\n    }\n  }\n};","map":{"version":3,"names":["name","data","pieces","searchText","selectedCategory","filterDisponible","sortOrder","panier","mounted","fetch","then","response","json","console","log","catch","error","computed","categories","set","Set","map","piece","categorie","Array","from","filteredPieces","filter","disponible","nom","toLowerCase","includes","sortedPieces","slice","sort","a","b","prix","totalPanier","reduce","total"],"sources":["/home/kyyblin/tp-auto/src/App.vue"],"sourcesContent":["\n<template>\n  <div id=\"app\">\n    <h1>E-commerce  Vente de pièces auto</h1>\n    \n    <!-- Zone de filtres -->\n    <div class=\"filters\">\n      <!-- Recherche en temps réel -->\n      <input\n        type=\"text\"\n        v-model=\"searchText\"\n        placeholder=\"Recherchez une pièce...\"\n      />\n\n      <!-- Filtrage par catégorie -->\n      <select v-model=\"selectedCategory\">\n        <option value=\"\">Toutes les catégories</option>\n        <option\n          v-for=\"categorie in categories\"\n          :key=\"categorie\"\n          :value=\"categorie\"\n        >\n          {{ categorie }}\n        </option>\n      </select>\n\n      <!-- Filtre par disponibilité -->\n      <label>\n        <input type=\"checkbox\" v-model=\"filterDisponible\" />\n        Disponible uniquement\n      </label>\n\n      <!-- Tri par prix -->\n      <select v-model=\"sortOrder\">\n        <option value=\"asc\">Prix croissant</option>\n        <option value=\"desc\">Prix décroissant</option>\n      </select>\n    </div>\n\n    <!-- Affichage des pièces -->\n    <div class=\"products\">\n     \n      <div v-for=\"piece in sortedPieces\" :key=\"piece.id\" class=\"product\">\n        <!-- Affichage de l'image (attention à bien placer vos images dans src/assets/) -->\n        \n        <h2>{{ piece.nom }}</h2>\n        <p>Prix : {{ piece.prix }}€</p>\n        <p>Catégorie : {{ piece.categorie }}</p>\n        <p v-if=\"piece.disponible\">En stock</p>\n        <p v-else>Indisponible</p>\n        <button\n          @click=\"ajouterAuPanier(piece)\"\n          :disabled=\"!piece.disponible\"\n        >\n          Ajouter au panier\n        </button>\n      </div>\n    </div>\n\n    <!-- Affichage du panier -->\n    <div class=\"panier\">\n      <h2>Panier</h2>\n      <div v-if=\"panier.length\">\n        <ul>\n          <li v-for=\"(item, index) in panier\" :key=\"index\">\n            {{ item.nom }}  {{ item.prix }}€\n          </li>\n        </ul>\n        <p>Total : {{ totalPanier }}€</p>\n      </div>\n      <p v-else>Votre panier est vide.</p>\n    </div>\n  </div>\n</template>\nimport pieces from '@/assets/pieces-auto.json'\n\n<script>\n// Importez les données depuis le fichier JSON\nexport default {\n  name: \"App\",\n  data() {\n    return {\n      pieces: [{}],\n      searchText: \"\",\n      selectedCategory: \"\",\n      filterDisponible: true, // Affiche par défaut uniquement les pièces disponibles\n      sortOrder: \"asc\",\n      panier: []\n    }\n  },\n  mounted() {\n  fetch('./asset/pieces-auto.json')\n    .then(response => response.json())\n    .then(data => {\n      console.log(data) ; \n      this.pieces = data;\n    })\n    .catch(error => console.error(\"Erreur de chargement des pièces:\", error));\n},\n  computed: {\n  // Liste des catégories uniques pour le filtre\n  categories() {\n    const set = new Set(this.pieces.map(piece => piece.categorie));\n    return Array.from(set);\n  },\n  // Filtrage des pièces en fonction de la recherche, de la catégorie et de la disponibilité\n  filteredPieces() {\n    return this.pieces.filter(piece => {\n      // Filtre sur la disponibilité\n      if (this.filterDisponible && !piece.disponible) return false;\n\n      // Filtre sur la barre de recherche (insensible à la casse)\n      if (\n        this.searchText &&\n        !piece.nom.toLowerCase().includes(this.searchText.toLowerCase())\n      ) {\n        return false;\n      }\n\n      // Filtre sur la catégorie sélectionnée\n      if (this.selectedCategory && piece.categorie !== this.selectedCategory) {\n        return false;\n      }\n\n      return true;\n    });\n  },\n  // Tri des pièces filtrées par prix\n  sortedPieces() {\n    return this.filteredPieces.slice().sort((a, b) => {\n      return this.sortOrder === \"asc\" ? a.prix - b.prix : b.prix - a.prix;\n    });\n  },\n  // Calcul du total du panier\n  totalPanier() {\n    return this.panier.reduce((total, piece) => total + piece.prix, 0);\n  }\n}\n}\n</script>\n\n<style>\n#app {\n  max-width: 1200px;\n  margin: auto;\n  font-family: Arial, sans-serif;\n  padding: 20px;\n}\n.filters {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 1em;\n  margin-bottom: 1em;\n  align-items: center;\n}\n.products {\n  display: flex;\n  flex-wrap: wrap;\n  gap: 1em;\n}\n.product {\n  border: 1px solid #ccc;\n  padding: 1em;\n  width: 200px;\n  text-align: center;\n}\n.panier {\n  margin-top: 2em;\n  border-top: 2px solid #333;\n  padding-top: 1em;\n}\n</style>\n"],"mappings":";;;;;;;;;;;AA6EA;AACA,eAAe;EACbA,IAAI,EAAE,KAAK;EACXC,IAAIA,CAAA,EAAG;IACL,OAAO;MACLC,MAAM,EAAE,CAAC,CAAC,CAAC,CAAC;MACZC,UAAU,EAAE,EAAE;MACdC,gBAAgB,EAAE,EAAE;MACpBC,gBAAgB,EAAE,IAAI;MAAE;MACxBC,SAAS,EAAE,KAAK;MAChBC,MAAM,EAAE;IACV;EACF,CAAC;EACDC,OAAOA,CAAA,EAAG;IACVC,KAAK,CAAC,0BAA0B,EAC7BC,IAAI,CAACC,QAAO,IAAKA,QAAQ,CAACC,IAAI,CAAC,CAAC,EAChCF,IAAI,CAACT,IAAG,IAAK;MACZY,OAAO,CAACC,GAAG,CAACb,IAAI;MAChB,IAAI,CAACC,MAAK,GAAID,IAAI;IACpB,CAAC,EACAc,KAAK,CAACC,KAAI,IAAKH,OAAO,CAACG,KAAK,CAAC,kCAAkC,EAAEA,KAAK,CAAC,CAAC;EAC7E,CAAC;EACCC,QAAQ,